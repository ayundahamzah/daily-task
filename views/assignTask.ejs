<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Assign Task</title>
  </head>
  <body><center>
    <table>
      <tr>
        <th>Employee</th>
        <th>Task</th>
        <th></th>
      </tr>
      <tr>
        <form class="" action="/users/assignTask" method="post">
          <td>
          <select class="" name="userId">
            <% dataUsers.forEach(function(user){ %>
              <option value="<%= user.id %>"><%= user.name %></option>
            <% }) %>
          </select>
          </td>
          <td>
          <select class="" name="taskId">
            <% dataTasks.forEach(function(task){ %>
              <option value="<%= task.id %>"><%= task.Task %></option>
            <% }) %>
          </select>
          </td>
          <td>
          <input type="submit" name="" value="Add">
          </td>
        </form>
      </tr>
    </table>
  </center></body>
</html>