<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Task Monitor</title>
  </head>
  <body><center>
    <h2>Monitor Task</h2>
    <table border='1' style = 'text-align: center'>
      <tr>
        <th>No</th>
        <th>Employee</th>
        <th>Task</th>
        <th>Status</th>
        <th>ETA</th>
      </tr>
      <% dataUserTasks.forEach(function(userTask){ %>
        <tr>
          <td></td>
          <td><%= userTask.User.name %></td>
          <td><%= userTask.Task.Task %></td>
          <td><%= userTask.status %></td>
          <td><%= userTask.Task.estimatedTimeOfWork %> minutes</td>
        </tr>
      <% }) %>
    </table>
  </center></body>
</html>